{
  "name": "node-art",
  "version": "1.0.0",
  "description": "Production-grade Node.js 24 LTS backend architecture with TypeScript, GraphQL, REST APIs, and enterprise features",
  "main": "dist/app.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=local tsx watch src/app.ts",
    "dev:env": "cross-env NODE_ENV=dev tsx watch src/app.ts",
    "build": "tsc",
    "start": "cross-env NODE_ENV=production node dist/app.js",
    "start:dev": "cross-env NODE_ENV=dev node dist/app.js",
    "start:ppd": "cross-env NODE_ENV=ppd node dist/app.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "docs:generate": "apidoc -i src/routes -o src/docs/apidoc",
    "prepare": "husky install"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "graphql",
    "rest-api",
    "jwt",
    "sequelize",
    "mongoose",
    "redis",
    "docker",
    "production"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "graphql": "^16.9.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.9.3",
    "mysql2": "^3.11.5",
    "sequelize": "^6.37.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "apidoc": "^1.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "sequelize-cli": "^6.6.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
